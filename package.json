    "gatsby-transformer-remark": "^2.1.1-rc.0",
    "marked": "^0.5.0",
    "netlify-cms": "^2.1.1",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-helmet": "^5.2.0",
    "slugify": "^1.3.1"
  },
  "devDependencies": {
    "multer": "^1.3.1",
    "prettier": "^1.14.2"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bjrn/gatsby-with-netlify-cms"
