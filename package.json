{
  "name": "gatsby-with-netlify-cms",
  "description": "Gatsby site using NetlifyCMS with local file-system api in dev mode",
  "version": "1.0.0",
  "scripts": {
    "build": "gatsby build",
    "start": "gatsby develop",
    "develop": "gatsby develop",
    "serve": "gatsby build && gatsby serve",
    "format": "prettier --write '**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf .cache && rm -rf public"
  },
  "dependencies": {
    "gatsby": "next",
    "gatsby-plugin-netlify-cms": "^3.0.0-rc.0",
    "gatsby-plugin-react-helmet": "next",
